{
	"name": "myaubot-twitch",
	"version": "1.0.0",
	"description": "A bot that catifies your chat",
	"main": "index.js",
	"dependencies": {
		"dotenv": "^6.2.0",
		"lodash": "^4.17.11",
		"lowdb": "^1.0.0",
		"twitch-js": "^1.2.17"
	},
	"devDependencies": {
		"eslint": "^5.10.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-plugin-prettier": "^3.0.0",
		"husky": "^1.3.0",
		"lint-staged": "^8.1.0",
		"prettier": "^1.15.3",
		"prettier-eslint": "^8.8.2"
	},
	"scripts": {
		"start": "node index.js",
		"test": "echo \"Error: no test specified\" && exit 1",
		"precommit": "lint-staged"
	},
	"lint-staged": {
		"**/*.js": [
			"prettier --write --single-quote --trailing-comma all",
			"git add"
		],
		"**/*.json": [
			"prettier --write --parser json",
			"git add"
		],
		"**/*.md": [
			"prettier --write --parser markdown",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaredkotoff/myaubot-twitch.git"
	},
	"author": "UpDownLeftDie",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/jaredkotoff/myaubot-twitch/issues"
	},
	"homepage": "https://github.com/jaredkotoff/myaubot-twitch#readme"
}
